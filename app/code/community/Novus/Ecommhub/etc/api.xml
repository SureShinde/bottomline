<config>
	<api>
		<resources>
			<ecommhub translate="title" module="ecommhub">
				<model>ecommhub/version_api</model>
				<title>Version Resource</title>
				<methods>
					<version translate="title" module="ecommhub">
						<title>Retrieve current version number</title>
						<acl>ecommhub/versioninfo</acl>
						<method>version</method>
					</version>
				</methods>
			</ecommhub>
			<ecommhub_catalog_product translate="title" module="ecommhub">
				<model>ecommhub/catalog_product_api</model>
				<methods>
					<info translate="title" module="ecommhub">
						<title>Retrieve product</title>
						<acl>ecommhub/catalog_product_info</acl>
						<method>info</method>
					</info>
					<list translate="title" module="ecommhub">
						<title>Retrieve product list</title>
						<acl>ecommhub/catalog_product_list</acl>
						<method>items</method>
					</list>
					<listcount translate="title" module="ecommhub">
						<title>Retrieve product list count</title>
						<acl>ecommhub/catalog_product_list</acl>
						<method>itemscount</method>
					</listcount>
				</methods>
			</ecommhub_catalog_product>
			<ecommhub_cataloginventory_stock_item translate="title" module="ecommhub">
				<model>ecommhub/catalogInventory_stock_item_api</model>
				<methods>
					<update translate="title" module="ecommhub">
						<title>Update</title>
						<acl>ecommhub/cataloginventory_stock_item_update</acl>
						<method>update</method>
					</update>
					<list translate="title" module="ecommhub">
						<title>Update</title>
						<acl>ecommhub/cataloginventory_stock_item_list</acl>
						<method>items</method>
					</list>
					<massupdate translate="title" module="ecommhub">
						<title>Massupdate product stock data</title>
						<acl>ecommhub/cataloginventory_stock_item_massupdate</acl>
						<method>massupdate</method>
					</massupdate>
				</methods>
			</ecommhub_cataloginventory_stock_item>
			<ecommhub_sales_order>
				<model>ecommhub/sales_order_api</model>
				<methods>
					<list translate="title" module="ecommhub">
						<title>Retrieve order list</title>
						<acl>ecommhub/sales_order_list</acl>
						<method>ecommhubitems</method>
					</list>
					<listcount translate="title" module="ecommhub">
						<title>Retrieve order list count</title>
						<acl>ecommhub/sales_order_list</acl>
						<method>ecommhubitemscount</method>
					</listcount>
				</methods>
			</ecommhub_sales_order>
			<ecommhub_sales_order_shipment>
				<model>ecommhub/sales_order_shipment_api</model>
				<methods>
					<addTrack translate="title" module="ecommhub">
						<title>Add shipment tracking and send email</title>
						<acl>ecommhub/sales_order_shipment_addtrack</acl>
						<method>addTrack</method>
					</addTrack>
				</methods>
			</ecommhub_sales_order_shipment>
		</resources>
		<v2>
			<resources_function_prefix>
				<ecommhub>ecommhub</ecommhub>
				<ecommhub_catalog_product>ecommhubCatalogProduct</ecommhub_catalog_product>
				<ecommhub_cataloginventory_stock_item>ecommhubCatalogInventoryStockItem</ecommhub_cataloginventory_stock_item>
				<ecommhub_sales_order>ecommhubSalesOrder</ecommhub_sales_order>
				<ecommhub_sales_order_shipment>ecommhubSalesOrderShipment</ecommhub_sales_order_shipment>
			</resources_function_prefix>
		</v2>
		<acl>
			<resources>
				<ecommhub translate="title" module="ecommhub">
					<title>Ecommhub ACL</title>
					<list translate="title" module="ecommhub">
						<title>List websites</title>
					</list>
					<versioninfo translate="title" module="ecommhub">
						<title>Retrieve Extension version information</title>
					</versioninfo>
					<catalog_product_info translate="title" module="ecommhub">
						<title>Update inventory data</title>
					</catalog_product_info>
					<catalog_product_list translate="title" module="ecommhub">
						<title>Update inventory data</title>
					</catalog_product_list>
					<cataloginventory_stock_item_update translate="title" module="ecommhub">
						<title>Update inventory data</title>
					</cataloginventory_stock_item_update>
					<cataloginventory_stock_item_list translate="title" module="ecommhub">
						<title>Update inventory data</title>
					</cataloginventory_stock_item_list>
					<cataloginventory_stock_item_massupdate translate="title" module="ecommhub">
						<title>Massupdate inventory data</title>
					</cataloginventory_stock_item_massupdate>
					<sales_order_list translate="title" module="ecommhub">
						<title>List sales orders</title>
					</sales_order_list>
					<sales_order_shipment_addtrack translate="title" module="ecommhub">
						<title>Add shipment tracking code and send email</title>
					</sales_order_shipment_addtrack>
				</ecommhub>
			</resources>
		</acl>
	</api>
</config>